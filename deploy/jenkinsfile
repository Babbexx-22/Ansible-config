pipeline {
    agent any

  stages {
    stage('Build') {
      steps {
        script {
          sh 'echo "Building Stage"'
        }
      }
    }

    stage ('Test') {
      steps {
        script {
          sh 'echo "Testing stage"'
        }
      }
    }

    stage ('Package') {
      steps {
        script{
          sh 'echo "Package stage"'
        }
      }
    }

    stage ('Deploy') {
      steps {
        script {
          sh 'echo "Deploy stage"'
        }
      }
    }

    stage ('Cleanup') {
      steps {
        script {
          sh 'echo "Cleanup stage"'
        }
      }
    }
    }
}
